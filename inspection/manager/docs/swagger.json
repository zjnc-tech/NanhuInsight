{
    "swagger": "2.0",
    "info": {
        "contact": {}
    },
    "paths": {
        "/inspection/api/v1/create_job": {
            "post": {
                "description": "创建新的作业任务，需提供作业详情以及相关请求头信息",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "作业管理"
                ],
                "summary": "创建作业任务",
                "parameters": [
                    {
                        "type": "string",
                        "description": "请求者的用户名，需进行 URL 编码",
                        "name": "x-username",
                        "in": "header",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "作业执行的集群名称",
                        "name": "x-cluster",
                        "in": "header",
                        "required": true
                    },
                    {
                        "description": "包含作业详细信息的 JSON 数据",
                        "name": "body",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/job.CreateJobRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "成功响应，返回创建的作业 ID",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    },
                    "400": {
                        "description": "参数无效或缺少必要字段",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    },
                    "500": {
                        "description": "服务器内部错误",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    }
                }
            }
        },
        "/inspection/api/v1/create_template": {
            "post": {
                "description": "创建新的模板，需提供模板详细信息以及相关请求头信息",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "模板管理"
                ],
                "summary": "创建模板",
                "parameters": [
                    {
                        "type": "string",
                        "description": "请求者的用户名，需进行 URL 编码",
                        "name": "x-username",
                        "in": "header",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "模板创建所在的集群名称",
                        "name": "x-cluster",
                        "in": "header",
                        "required": true
                    },
                    {
                        "description": "包含模板详细信息的 JSON 数据",
                        "name": "body",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/templates.CreateRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "成功返回创建模板的响应",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    },
                    "400": {
                        "description": "请求错误，缺少必要参数或参数无效",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    },
                    "500": {
                        "description": "服务器内部错误",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    }
                }
            }
        },
        "/inspection/api/v1/delete_template": {
            "delete": {
                "description": "删除指定集群中的模板",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "模板管理"
                ],
                "summary": "删除模板",
                "parameters": [
                    {
                        "type": "string",
                        "description": "模板所在集群的名称",
                        "name": "x-cluster",
                        "in": "header",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "要删除的模板名称",
                        "name": "template",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "成功返回模板删除的响应",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    },
                    "400": {
                        "description": "请求错误，缺少必要参数或参数无效",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    },
                    "500": {
                        "description": "服务器内部错误",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    }
                }
            }
        },
        "/inspection/api/v1/download_log": {
            "post": {
                "description": "下载指定作业中的检查结果的日志文件",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "检查结果"
                ],
                "summary": "下载测试用例日志",
                "parameters": [
                    {
                        "description": "包含 jobId 和 caseName 的 JSON 对象",
                        "name": "body",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/controllers.DownloadRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "指定测试用例的日志文件",
                        "schema": {
                            "type": "file"
                        }
                    },
                    "400": {
                        "description": "请求错误，缺少或无效的参数",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    },
                    "404": {
                        "description": "未找到日志文件",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    },
                    "500": {
                        "description": "服务器内部错误",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    }
                }
            }
        },
        "/inspection/api/v1/get_all_cases": {
            "get": {
                "description": "根据指定的模式检索脚本列表",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "脚本管理"
                ],
                "summary": "获取脚本",
                "parameters": [
                    {
                        "type": "string",
                        "description": "集群名称，指定脚本所在的集群",
                        "name": "x-cluster",
                        "in": "header",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "过滤检查项的模式，根据指定的模式筛选",
                        "name": "mode",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "资源名称，根据指定的资源筛选检查项",
                        "name": "resource",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "成功返回包含脚本列表的响应",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    },
                    "400": {
                        "description": "请求错误，例如缺少必要参数或参数无效",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    },
                    "500": {
                        "description": "服务器内部错误",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    }
                }
            }
        },
        "/inspection/api/v1/get_all_templates": {
            "get": {
                "description": "根据集群名称、模式和资源名称查询模板列表",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "模板管理"
                ],
                "summary": "根据模式获取模板",
                "parameters": [
                    {
                        "type": "string",
                        "description": "集群名称，用于过滤模板（必填项）",
                        "name": "x-cluster",
                        "in": "header",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "模式，用于过滤模板（必填项）",
                        "name": "mode",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "资源名称，用于过滤模板（可选项）",
                        "name": "resource",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "成功返回模板名称列表的响应",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    },
                    "400": {
                        "description": "请求错误，缺少必要参数或参数无效",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    },
                    "500": {
                        "description": "服务器内部错误",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    }
                }
            }
        },
        "/inspection/api/v1/get_nodes_from_cluster": {
            "get": {
                "description": "根据集群名称和模式，获取节点（IP 地址）列表，并根据资源名返回相关节点信息",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "节点管理"
                ],
                "summary": "获取集群节点",
                "parameters": [
                    {
                        "type": "string",
                        "description": "集群名称，用于指定要查询节点的集群",
                        "name": "x-cluster",
                        "in": "header",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "过滤模式，用于根据特定条件筛选节点",
                        "name": "mode",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "资源名称，用于指定要查询的节点",
                        "name": "resource",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "成功返回包含节点 IP 地址列表的响应",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    },
                    "400": {
                        "description": "请求错误，例如缺少必要参数（如集群名称、模式或资源）",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    },
                    "500": {
                        "description": "服务器内部错误",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    }
                }
            }
        },
        "/inspection/api/v1/get_template_detail": {
            "get": {
                "description": "根据集群名称和模板名称查询模板的详细信息",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "模板管理"
                ],
                "summary": "获取模板详情",
                "parameters": [
                    {
                        "type": "string",
                        "description": "集群名称，用于过滤模板（必填项）",
                        "name": "x-cluster",
                        "in": "header",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "模板名称（必填项）",
                        "name": "template",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "成功返回模板详情的响应",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    },
                    "400": {
                        "description": "请求错误，缺少必要参数或参数无效",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    },
                    "500": {
                        "description": "服务器内部错误",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    }
                }
            }
        },
        "/inspection/api/v1/get_template_info": {
            "get": {
                "description": "根据查询条件、分页参数和排序选项返回模板记录",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "模板管理"
                ],
                "summary": "获取所有模板",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "分页页码，默认值为 1",
                        "name": "pageID",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "description": "每页记录数，默认值为 10",
                        "name": "pageSize",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "根据集群名称过滤模板记录",
                        "name": "x-cluster",
                        "in": "header"
                    },
                    {
                        "type": "string",
                        "description": "根据模式过滤",
                        "name": "mode",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "根据定时任务状态过滤",
                        "name": "cron",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "排序字段，默认值为 'create_time'",
                        "name": "sortBy",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "排序顺序，可选值为 'asc' 或 'desc'，默认值为 'desc'",
                        "name": "sortOrder",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "成功返回模板记录列表的响应",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    },
                    "500": {
                        "description": "服务器内部错误",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    }
                }
            }
        },
        "/inspection/api/v1/job/detail": {
            "get": {
                "description": "根据作业 ID 获取作业的详细信息",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "作业管理"
                ],
                "summary": "获取作业详情",
                "parameters": [
                    {
                        "type": "string",
                        "description": "集群名称",
                        "name": "x-cluster",
                        "in": "header",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "需要获取详情的作业 ID",
                        "name": "jobID",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "成功返回作业的详细信息",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    },
                    "400": {
                        "description": "x-cluster 或 jobID 为必填项",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    },
                    "500": {
                        "description": "获取作业详情失败",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    }
                }
            }
        },
        "/inspection/api/v1/job/retry": {
            "post": {
                "description": "根据作业 ID 重试一个作业，从数据库中获取作业详情，并使用相同的参数创建新作业",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "作业管理"
                ],
                "summary": "重试作业",
                "parameters": [
                    {
                        "type": "string",
                        "description": "发起请求的用户名（需进行 URL 编码）",
                        "name": "x-username",
                        "in": "header",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "与作业关联的集群名称",
                        "name": "x-cluster",
                        "in": "header",
                        "required": true
                    },
                    {
                        "description": "包含需要重试的作业 ID 的 JSON 对象",
                        "name": "jobId",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "object"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "重试成功的响应，包含作业详情",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    },
                    "400": {
                        "description": "x-username 和 x-cluster 为必填项",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    },
                    "500": {
                        "description": "获取作业 ID 失败",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    }
                }
            }
        },
        "/inspection/api/v1/modify_template": {
            "patch": {
                "description": "修改模板的基本信息或定时任务配置",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "模板管理"
                ],
                "summary": "修改模板",
                "parameters": [
                    {
                        "type": "string",
                        "description": "请求者的用户名，需进行 URL 编码",
                        "name": "x-username",
                        "in": "header",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "模板所在集群的名称",
                        "name": "x-cluster",
                        "in": "header",
                        "required": true
                    },
                    {
                        "description": "包含模板修改信息的 JSON 数据",
                        "name": "body",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/templates.CreateRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "成功返回模板修改的响应",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    },
                    "400": {
                        "description": "请求错误，缺少必要参数或参数无效",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    },
                    "500": {
                        "description": "服务器内部错误",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    }
                }
            }
        },
        "/inspection/api/v1/node/download_excel": {
            "get": {
                "description": "下载节点表格模板",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/octet-stream"
                ],
                "tags": [
                    "节点管理"
                ],
                "summary": "下载 Excel 模板文件",
                "responses": {
                    "200": {
                        "description": "成功节点 Excel 文件",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "400": {
                        "description": "请求错误，例如文件不存在",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    },
                    "500": {
                        "description": "服务器内部错误，例如文件路径读取失败",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    }
                }
            }
        },
        "/inspection/api/v1/node/resource": {
            "get": {
                "description": "根据集群名称获取集群资源列表",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "节点管理"
                ],
                "summary": "获取集群资源",
                "parameters": [
                    {
                        "type": "string",
                        "description": "集群名称，用于指定要查询节点的集群",
                        "name": "x-cluster",
                        "in": "header",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "成功返回包含节点 IP 地址列表的响应",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    },
                    "400": {
                        "description": "请求错误，例如缺少必要参数（如集群名称或模式）或参数无效",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    },
                    "500": {
                        "description": "服务器内部错误",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    }
                }
            }
        },
        "/inspection/api/v1/node/upload_excel": {
            "post": {
                "description": "上传包含 IP 地址列表的 Excel 文件，与指定集群的节点进行比较，并返回匹配和未匹配结果。",
                "consumes": [
                    "multipart/form-data"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "节点管理"
                ],
                "summary": "上传 Excel 文件",
                "parameters": [
                    {
                        "type": "string",
                        "description": "集群名称，用于指定要比较的集群",
                        "name": "x-cluster",
                        "in": "header",
                        "required": true
                    },
                    {
                        "type": "file",
                        "description": "上传的 Excel 文件（支持 .xlsx 和 .xls 格式）",
                        "name": "file",
                        "in": "formData",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "过滤模式，用于指定例行还是深度检查",
                        "name": "mode",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "资源名称，用于指定要比较的资源",
                        "name": "resource",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "成功返回匹配的 IP 列表和匹配统计信息",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
                    },
                    "400": {
                        "description": "请求错误，例如缺少集群名称、文件参数或资源名称",
                        "schema": {
                            "$ref": "#/definitions/api.CommonResponse"
                        }
